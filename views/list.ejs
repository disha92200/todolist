<%- include("headertodo"); -%>


    <div class="box" id="headng">
        <h1 style="background-color: #A683E3">
            <%= listtitle %>
        </h1>
    </div>

    <div class="box">
        <% for(var i=0;i<newListItems.length; i++ ){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%=newListItems[i] %>
                </p>
            </div>
        <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="newitems" placeholder="new item" autocomplete="off">
            <button type="submit" name="list" value=<%=listtitle %>>+</button>
        </form>
    </div>
   

    <%- include("footertodo"); -%>